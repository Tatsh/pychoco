    <title type="text">logparser.lizardgui</title>
    <summary type="text">View and analyze your Log Files</summary>
    <updated>2025-10-22T03:49:55Z</updated>
    <author>
      <name>Lizard Labs</name>
    </author>
    <link rel="edit-media" title="V2FeedPackage" href="Packages(Id='logparser.lizardgui',Version='6.8')/$value" />
